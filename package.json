{
  "name": "mastdatabase-api-proxy-services",
  "version": "1.0.0",
  "private": true,
  "main": "src/server.ts",
  "prettier": "@flarum/prettier-config",
  "type": "module",
  "scripts": {
    "test": "tap \"test/**/*.test.cjs\"",
    "clean": "rimraf ./lib",
    "build": "yarn clean && tsc -p tsconfig.json",
    "watch": "yarn clean && tsc -w -p tsconfig.json",
    "start": "ts-node --esm --transpile-only src/server",
    "dev": "nodemon src/server",
    "format": "prettier --write .",
    "format-check": "prettier --check ."
  },
  "author": "David Wheatley <david@davwheat.dev> (https://davwheat.dev)",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cors": "^8.2.1",
    "@fastify/sensible": "^5.2.0",
    "@flarum/prettier-config": "^1.0.0",
    "fastify": "^4.15.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.5.0",
    "node-fetch": "^3.3.1",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.12",
    "@types/uuid": "^9.0.1",
    "json-schema-to-ts": "^2.8.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "tap": "^16.3.4",
    "typescript": "^5.0.4"
  },
  "packageManager": "yarn@3.5.0"
}
